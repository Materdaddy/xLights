<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="xChannelsFrame">
		<title>xChannels</title>
		<icon>..\include\xlights.ico</icon>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="yes">
					<size>750,400</size>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<option>1</option>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuImport" variable="MenuImport" member="yes">
					<label>Import</label>
					<handler function="OnMenuImportSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuExport" variable="MenuExport" member="yes">
					<label>Export</label>
					<handler function="OnMenuExportSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuSave" variable="MenuItemSave" member="yes">
					<label>Save</label>
					<handler function="OnMenuItemSaveSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>&amp;Channels</label>
				<object class="wxMenuItem" name="ID_MENUITEM_ADDNETWORK" variable="MenuAddNetwork" member="yes">
					<label>Add Network</label>
					<handler function="OnMenuAddNetworkSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_SETLASTCHANNEL" variable="MenuSetLastChannel" member="yes">
					<label>Set Last Channel</label>
					<handler function="OnMenuSetLastChannelSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_ADDCONTROLLERS" variable="MenuAddControllers" member="yes">
					<label>Add Controllers</label>
					<handler function="OnMenuAddControllersSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuDelete" variable="MenuDeleteController" member="yes">
					<label>Delete Controller</label>
					<handler function="OnMenuDeleteControllerSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
